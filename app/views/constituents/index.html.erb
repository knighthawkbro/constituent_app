<p id="notice"><%= notice %></p>

<h1>Constituents</h1>

<table>
  <thead>
    <tr>
      <th>Member name</th>
      <th>Issue</th>
      <th>Status</th>
      <th>Contact info</th>
      <th>Psi</th>
      <th>Contact</th>
      <th>Assigned to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @constituents.each do |constituent| %>
      <tr>
        <td><%= constituent.member_name %></td>
        <td><%= constituent.issue %></td>
        <td><%= constituent.status %></td>
        <td><%= constituent.contact_info %></td>
        <td><%= constituent.psi %></td>
        <td><%= constituent.contact_id %></td>
        <td><%= constituent.assigned_to %></td>
        <td><%= link_to 'Show', constituent %></td>
        <td><%= link_to 'Edit', edit_constituent_path(constituent) %></td>
        <td><%= link_to 'Destroy', constituent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Constituent', new_constituent_path %>
