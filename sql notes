table constituents
  constituent_id Integer
  member_name Text
  issue text
  status string
  contact_information text
  psi boolean
  contact_id integer
  date_created date & time
  assigned_to string

table notes
  note_id integer
  note text
  constituent_id integer
  Date_Created date & time
  Date_Updated date & time
  cca string

Table contacts
  contact_id number
  name Text
  office text
  phone text

Table calls
  constituent_id
  contact_id
